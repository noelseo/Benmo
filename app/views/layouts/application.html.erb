<!DOCTYPE html>
<html>
  <head>
    <title>Benmo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <Link 
    rel="icon" 
    type="image/icon" 
    href="https://raw.githubusercontent.com/noelseo/Benmo/master/app/assets/images/benmo.ico"
    >

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script type="text/javascript">
      window.phonesURL = "<%= image_url('phones.mov') %>";
    </script>

    <script type="text/javascript">
      window.benmologo = "<%= image_url('benmo-logo.png') %>";
    </script>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
